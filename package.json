{
  "name": "one-reference-app",
  "description": "ONE Reference App",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:api\" \"pnpm run dev:web\"",
    "dev:web": "vite",
    "dev:api": "tuono dev",
    "dev:desktop": "TAURI_PLATFORM=1 tauri dev",
    "build": "pnpm run build:api && pnpm run build:web",
    "build:web": "vite build",
    "build:api": "tuono build",
    "build:desktop": "TAURI_PLATFORM=1 vite build --config vite.config.ts && tauri build",
    "start": "cargo run --release",
    "preview": "vite preview",
    "preview:tauri": "vite preview --config vite.tauri.config.ts",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist build-tauri .tuono/build node_modules/.vite",
    "deps": "pnpm install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.19",
